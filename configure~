EOF
("$JAVAC" -Xlint:divzero conftest.java 2>&5) \
  && rm -f conftest.class
then :
  emacs_cv_javac_knows_lint_divzero=yes
else $as_nop
  emacs_cv_javac_knows_lint_divzero=no
fi
