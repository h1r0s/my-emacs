    WEBP_CFLAGS="$ndk_import_includes $WEBP_CFLAGS"
  done
fi

  done

  if test "$ndk_found" = "yes"
then :
  HAVE_WEBP=yes
else $as_nop
  HAVE_WEBP=no
fi

else $as_nop

pkg_failed=no
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for $WEBP_MODULE" >&5
printf %s "checking for $WEBP_MODULE... " >&6; }

