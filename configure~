       printf "%s\n" "#define PTY_OPEN fd = getpt ()" >>confdefs.h

fi
    ;;

  gnu-linux | gnu-kfreebsd | dragonfly | freebsd | openbsd | netbsd | darwin | nacl )
        if test "x$ac_cv_func_grantpt" = xyes; then

