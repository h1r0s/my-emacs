if test "${with_kerberos5}" != no; then
  if test "${with_kerberos}" = no; then
    with_kerberos=yes
    printf "%s\n" "#define KERBEROS 1" >>confdefs.h

  fi

printf "%s\n" "#define KERBEROS5 1" >>confdefs.h

