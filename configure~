		      test "x$?" != "x0" && pkg_failed=yes
else
  pkg_failed=yes
fi
 else
    pkg_failed=untried
fi
if test -n "$LIBSYSTEMD_LIBS"; then
    pkg_cv_LIBSYSTEMD_LIBS="$LIBSYSTEMD_LIBS"
 elif test -n "$PKG_CONFIG"; then
