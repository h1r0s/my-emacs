printf "%s\n" "$emacs_cv_signals_via_chars" >&6; }

    test "$emacs_cv_signals_via_chars" = yes &&
      printf "%s\n" "#define SIGNALS_VIA_CHARACTERS 1" >>confdefs.h

    ;;
esac




