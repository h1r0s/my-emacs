  $EGREP "yes" >/dev/null 2>&1
then :
  gl_cv_header_errno_h_ENOLINK=yes
else $as_nop
  gl_cv_header_errno_h_ENOLINK=no
fi
rm -rf conftest*

      if test $gl_cv_header_errno_h_ENOLINK = no; then
        cat confdefs.h - <<_ACEOF >conftest.$ac_ext
