        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking whether pselect detects invalid fds" >&5
printf %s "checking whether pselect detects invalid fds... " >&6; }
if test ${gl_cv_func_pselect_detects_ebadf+y}
then :
  printf %s "(cached) " >&6
