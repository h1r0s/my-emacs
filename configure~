{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $emacs_cv_usable_SIGIO" >&5
printf "%s\n" "$emacs_cv_usable_SIGIO" >&6; }
    if test $emacs_cv_usable_SIGIO = yes; then

printf "%s\n" "#define USABLE_SIGIO 1" >>confdefs.h

