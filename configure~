  func_gl_gnulib_m4code_getdtablesize ()
  {
    if $gl_gnulib_enabled_getdtablesize; then :; else





  if test $ac_cv_func_getdtablesize = yes &&
     test $ac_cv_have_decl_getdtablesize = yes; then
