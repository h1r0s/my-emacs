  CAIRO_MODULE="cairo >= $CAIRO_REQUIRED"
  if test -n "$ndk_INITIALIZED"
then :

  ndk_modules=
  ndk_parse_pkg_config_string "$CAIRO_MODULE"
  ndk_found=no

