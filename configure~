printf "%s\n" "yes" >&6; }
	GTK_CFLAGS=`printf "%s\n" "$GTK_CFLAGS" | sed -e "$edit_cflags"`
        pkg_check_gtk=yes
fi
fi
    if test "$pkg_check_gtk" = "no" && test "$with_gtk3" = "yes"; then
       as_fn_error $? "$GTK_PKG_ERRORS" "$LINENO" 5
    fi
    if test "$pkg_check_gtk" = "yes"; then

