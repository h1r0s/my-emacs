ac_compiler_gnu=$ac_cv_cxx_compiler_gnu

   ac_fn_cxx_check_header_compile "$LINENO" "app/Application.h" "ac_cv_header_app_Application_h" "$ac_includes_default"
if test "x$ac_cv_header_app_Application_h" = xyes
then :
