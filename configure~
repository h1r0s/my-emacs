/* end confdefs.h.  */
$ac_includes_default
int
main (void)
{
int size = getgroups (0, 0);
            gid_t *list = malloc (size * sizeof *list);
            int result = getgroups (-1, list) != -1;
            free (list);
            return result;
  ;
  return 0;
}
