# Remove any trailing slashes in these variables.
case $prefix in
  */) prefix=`$as_dirname -- "$prefix." ||
$as_expr X"$prefix." : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X"$prefix." : 'X\(//\)[^/]' \| \
	 X"$prefix." : 'X\(//\)$' \| \
	 X"$prefix." : 'X\(/\)' \| . 2>/dev/null ||
printf "%s\n" X"$prefix." |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`;;
esac
case $exec_prefix in
  */) exec_prefix=`$as_dirname -- "$exec_prefix." ||
$as_expr X"$exec_prefix." : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X"$exec_prefix." : 'X\(//\)[^/]' \| \
	 X"$exec_prefix." : 'X\(//\)$' \| \
	 X"$exec_prefix." : 'X\(/\)' \| . 2>/dev/null ||
printf "%s\n" X"$exec_prefix." |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`;;
esac

