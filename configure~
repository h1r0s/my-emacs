        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: yes" >&5
printf "%s\n" "yes" >&6; }
	FREETYPE_CFLAGS=`printf "%s\n" "$FREETYPE_CFLAGS" | sed -e "$edit_cflags"`
        HAVE_FREETYPE=yes
fi
fi

       test "$HAVE_FREETYPE" = "no" &&
	 as_fn_error $? "libxft requires libfreetype" "$LINENO" 5
    fi
  fi				  # $HAVE_CAIRO != yes

