  # before ndk_MAKEFILES; likewise for its includes.
  ndk_import_includes=
  for ndk_module in $module_imports; do
    ndk_resolve_import_module $ndk_module
    FREETYPE_CFLAGS="$ndk_import_includes $FREETYPE_CFLAGS"
  done
fi

  done

  if test "$ndk_found" = "yes"
then :
  HAVE_FREETYPE=yes
else $as_nop
  HAVE_FREETYPE=no
fi

else $as_nop

