        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: yes" >&5
printf "%s\n" "yes" >&6; }
	FONTCONFIG_CFLAGS=`printf "%s\n" "$FONTCONFIG_CFLAGS" | sed -e "$edit_cflags"`
        HAVE_FONTCONFIG=yes
fi
fi

    test "$HAVE_FONTCONFIG" = "no" &&
      as_fn_error $? "cairo requires libfontconfig" "$LINENO" 5
        HAVE_XFT=no
  else
